{
  "name": "gradient-script",
  "version": "0.2.0",
  "description": "Symbolic differentiation for structured types with a simple DSL",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gradient-script": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "symbolic",
    "differentiation",
    "gradients",
    "structured-types",
    "vector",
    "autodiff",
    "code-generation"
  ],
  "author": "Mattias Fagerlund <mattias.fagerlund@carretera.se>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfagerlund/gradient-script.git"
  },
  "bugs": {
    "url": "https://github.com/mfagerlund/gradient-script/issues"
  },
  "homepage": "https://github.com/mfagerlund/gradient-script#readme",
  "files": [
    "dist/",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:verbose": "cross-env VERBOSE=true vitest run",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "npm run build"
  }
}
